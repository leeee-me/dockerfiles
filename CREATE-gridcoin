# Setup /var/lib/boinc-client via boinc-client in native or docker

# Prepare /home/me/.GridcoinResearch/ with wallet-backup tarball

docker build -t gridcoin -f Dockerfile.arm64v8-gridcoinresearchd .

docker run -d --name gridcoin --net=host  --pid=host --security-opt apparmor=unconfined -e TZ="Asia/Taipei" --restart unless-stopped -v /opt/appdata/boinc:/var/lib/boinc-client -v /home/leeee:/home/me gridcoin
